{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "abraham/twitteroauth": "^3.1",
        "ahmedsaoud31/laravel-permission-to-vuejs": "dev-master",
        "anhskohbo/no-captcha": "^3.3",
        "benjamincrozat/laravel-dropbox-driver": "^3.0",
        "doctrine/dbal": "^3.0",
        "fakerphp/faker": "^1.23",
        "guzzlehttp/guzzle": "^7.2.1",
        "intervention/image": "^2.7",
        "laravel/cashier": "^15.0",
        "laravel/framework": "^11.0",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "^5.0",
        "laravel/passport": "^12.0",
        "laravel/pulse": "^1.2",
        "laravel/reverb": "@beta",
        "laravel/sanctum": "^4.0",
        "laravel/slack-notification-channel": "^3.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "predis/predis": "^2.2",
        "pusher/pusher-php-server": "^7.0",
        "spatie/emoji": "^4.0",
        "spatie/laravel-backup": "^8.0",
        "spatie/laravel-permission": "^6.0",
        "stripe/stripe-php": "^13.0",
        "youthage/laravel-geohash": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "coreproc/laravel-debugbar-git-info": "^0.1.1",
        "driftingly/rector-laravel": "^1.2",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "files": [
            "app/Helpers/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
